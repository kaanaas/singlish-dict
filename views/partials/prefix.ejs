<hr class="hr-pale">

<div class="result">
    <div class="not-found">
        <h4><%= typeof headerText != 'undefined' ? headerText : "entries starting with" %></h4>
        <h3>'<%= searchInput %>'</h3>
    </div>
    <ul class="prefix-list">
    <% result = result.sort(); %>
    <% for(let i = 0; i < result.length; i++){ %>    
        <li><a href="./?q=<%= result[i] %>"><%= result[i] %></a></li>
    <% } %>
    </ul>
</div>